$('.notification-list').append('<%= j render partial: "notification_show_all",
  locals: {notifications: @warnings} %>')
<% if @warnings.current_page == @warnings.total_pages %>
  $('#view-more').remove();
<% else %>
  $('#view-more a').attr('href', '<%= url_for(page: @warnings.next_page) %>');
<% end %>
