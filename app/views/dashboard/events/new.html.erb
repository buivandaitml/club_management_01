<div id="page-content">
  <ul class="breadcrumb breadcrumb-top">
    <li><%= t "admin" %></li>
    <li>
      <%= link_to t("club_manager.event.new"),
        new_dashboard_club_event_path %>
    </li>
  </ul>
  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-10">
      <div class="block">
        <div class="block-title">
          <h2><strong><%= @club.name %></strong></h2>
        </div>
        <%= link_to t("back"), dashboard_club_path(@club.id),
          class: "btn btn-danger btn-sm" %>
        <%= render "form_club_event" %>
      </div>
    </div>
    <div class="col-md-1"></div>
  </div>
</div>
