<% if @club.ratings.find_by(user: current_user) %>
  $('#page-container').append('<%= j render "shared/flash_warning" %>');
<% else %>
  $('.rating-container').html('<%= j render "rating" %>');
  $('.rating').rating();
  $('.num-rate').text('<%= @club.rating %>');
  $('.glyphicon').css('pointer-events', 'none');
  $('#page-container').append('<%= j render "shared/flash_warning" %>');
<% end %>
